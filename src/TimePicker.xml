<?xml version="1.0" encoding="utf-8"?>
<widget id="mendix.timepicker.TimePicker" needsEntityContext="true" offlineCapable="true" pluginWidget="true" labeled="true" 
    xmlns="http://www.mendix.com/widget/1.0/" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../xsd/widget.xsd">
    <name>Time picker</name>
    <description>Time picker widget</description>
    <icon></icon>
    <properties>
        <property key="inputValue" type="attribute" required="true">
            <caption>Attribute</caption>
            <category>General</category>
            <description />
            <attributeTypes>
                <attributeType name="DateTime" />
            </attributeTypes>
        </property>
        <property key="editable" type="enumeration" defaultValue="default">
            <caption>Editable</caption>
            <category>General</category>
            <description />
            <enumerationValues>
                <enumerationValue key="default">Default</enumerationValue>
                <enumerationValue key="never">Never</enumerationValue>
            </enumerationValues>
        </property>
        <property key="timeFormat" type="enumeration" defaultValue="minutes">
            <caption>Time format</caption>
            <category>General</category>
            <description />
            <enumerationValues>
                <enumerationValue key="minutes">Hours, minutes</enumerationValue>
                <enumerationValue key="seconds">Hours, minutes, seconds</enumerationValue>
            </enumerationValues>
        </property>
        <property key="placeholder" type="translatableString" required="false">
            <caption>Placeholder text</caption>
            <category>General</category>
            <description />
        </property>
        <property key="validationMessage" type="translatableString" required="false">
            <caption>Validation message</caption>
            <category>General</category>
            <description />
        </property>
        <property key="onChange" type="action">
            <caption>On change action</caption>
            <category>Events</category>
            <description />
        </property>
        <property key="onEnter" type="action">
            <caption>On enter action</caption>
            <category>Events</category>
            <description />
        </property>
        <property key="onLeave" type="action">
            <caption>On leave action</caption>
            <category>Events</category>
            <description />
        </property>
    </properties>
</widget>
