<?xml version="1.0" encoding="utf-8"?>
<widget id="mendix.timepicker.TimePicker" needsEntityContext="true" offlineCapable="true" pluginWidget="true" xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../xsd/widget.xsd">
    <name>Time picker</name>
    <description>Time picker widget</description>
    <icon>
        
    </icon>
    <properties>
        <property key="timeFormat" type="enumeration" defaultValue="minutes">
            <caption>Time format</caption>
            <category>General</category>
            <description />
            <enumerationValues>
                <enumerationValue key="minutes">Minutes</enumerationValue>
                <enumerationValue key="seconds">Seconds</enumerationValue>
                <enumerationValue key="milliseconds">Milliseconds</enumerationValue>
            </enumerationValues>
        </property>
        <property key="timeNotation" type="enumeration" defaultValue="h12">
            <caption>Time notation</caption>
            <category>General</category>
            <description />
            <enumerationValues>
                <enumerationValue key="h12">12-hour</enumerationValue>
                <enumerationValue key="h24">24-hour</enumerationValue>
            </enumerationValues>
        </property>
        <property key="inputValue" type="attribute" required="true">
            <caption>Attribute (path)</caption>
            <category>General</category>
            <description />
            <attributeTypes>
                <attributeType name="DateTime" />
            </attributeTypes>
        </property>
        <property key="editable" type="enumeration" defaultValue="default">
            <caption>Editable</caption>
            <category>General</category>
            <description />
            <enumerationValues>
                <enumerationValue key="default">Default</enumerationValue>
                <enumerationValue key="never">Never</enumerationValue>
            </enumerationValues>
        </property>
        <property key="placeholder" type="translatableString" required="false">
            <caption>Placeholder text</caption>
            <category>General</category>
            <description />
        </property>
        <property key="hoursStep" type="integer" defaultValue="1">
            <caption>Hours step</caption>
            <category>Constraints</category>
            <description />
        </property>
        <property key="minutesStep" type="integer" defaultValue="1">
            <caption>Minutes step</caption>
            <category>Constraints</category>
            <description />
        </property>
        <property key="secondsStep" type="integer" defaultValue="1">
            <caption>Seconds step</caption>
            <category>Constraints</category>
            <description />
        </property>
        <property key="showLabel" type="boolean" defaultValue="true">
            <caption>Show label</caption>
            <category>Label</category>
            <description />
        </property>
        <property key="labelCaption" type="string" required="false" defaultValue="Time">
            <caption>Label caption</caption>
            <category>Label</category>
            <description />
        </property>
        <property key="labelOrientation" type="enumeration" defaultValue="vertical">
            <caption>Label orientation</caption>
            <category>Label</category>
            <description/>
            <enumerationValues>
                <enumerationValue key="horizontal">Horizontal</enumerationValue>
                <enumerationValue key="vertical">Vertical</enumerationValue>
            </enumerationValues>
        </property>
        <property key="labelWidth" type="integer" defaultValue="3">
            <caption>Horizontal label width</caption>
            <category>Label</category>
            <description>The width of the horizontal label, which must be a value between 1 and 10</description>
        </property>
        <property key="onChange" type="action" required="false">
            <caption>On change</caption>
            <category>Events</category>
            <description />
        </property>
        <property key="onEnter" type="action" required="false">
            <caption>On enter</caption>
            <category>Events</category>
            <description />
        </property>
        <property key="onLeave" type="action" required="false">
            <caption>On leave</caption>
            <category>Events</category>
            <description />
        </property>
    </properties>
</widget>
