{
  "name": "timepicker",
  "widgetName": "TimePicker",
  "version": "1.0.0",
  "description": "Time picker widget",
  "copyright": "Mendix 2018",
  "author": "jobvs",
  "config": {
    "widgetPath": "..//widgets",
    "projectPath": "..//"
  },
  "packagePath": "mendix",
  "scripts": {
    "start": "webpack-dev-server --hot --open",
    "emit": "tsc",
    "pretest": "tsc && npm run lint && npm run build",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:e2e": "",
    "lint": "tslint -c tslint.json '**/*.ts' '**/*.tsx' --exclude '**/node_modules/**'",
    "dev": "gulp --gulpfile Gulpfile.js",
    "build": "NODE_ENV=production gulp build --gulpfile Gulpfile.js",
    "lint:fix": "npm run lint -- --fix",
    "deploy": " npm run updateProject && npm run deployApp",
    "updateProject": "node ./node_modules/mendix-widget-build-script/dist/UpdateMxProject.js",
    "deployApp": "node ./node_modules/mendix-widget-build-script/dist/DeployMxApp.js",
    "deployAndTest": "npm run deploy && npm run test:e2e",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "pre-commit": [
    "emit",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mendixlabs/time-picker"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^4.1.1",
    "@storybook/addon-actions": "^4.1.1",
    "@storybook/addon-knobs": "^4.1.1",
    "@storybook/addons": "^4.1.1",
    "@storybook/react": "^4.1.1",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "@types/storybook__addon-a11y": "^3.3.1",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__react": "^4.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "fs-extra": "^7.0.1",
    "gulp": "^3.9.1",
    "gulp-help": "^1.6.1",
    "gulp-sequence": "^1.0.0",
    "gulp-watch": "^5.0.1",
    "gulp-zip": "^4.2.0",
    "jest": "^23.6.0",
    "mendix-client": "^7.15.6",
    "mendix-widget-build-script": "^1.0.3",
    "moment-locales-webpack-plugin": "^1.0.7",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.6.0",
    "react-test-renderer": "^16.6.3",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "moment": "^2.23.0",
    "react-datetime": "^2.16.3"
  }
}
