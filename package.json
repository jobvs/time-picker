{
    "name": "timepicker",
    "widgetName": "TimePicker",
    "version": "1.0.0",
    "description": "Time picker widget",
    "copyright": "Mendix 2018",
    "author": "jobvs",
    "config": {
        "widgetPath": "..//widgets",
        "projectPath": "..//"
    },
    "packagePath": "mendix",
    "scripts": {
        "start": "npm run storybook",
        "widget:watch": "webpack-dev-server --hot --open",
        "widget:dev": "gulp --gulpfile Gulpfile.js",
        "widget:build": "NODE_ENV=production gulp build --gulpfile Gulpfile.js",
        "pretest": "tsc && npm run lint && npm run widget:build",
        "test": "npm run unit && npm run e2e",
        "test:ci": "npm run unit:ci && npm run e2e",
        "unit": "jest",
        "unit:ci": "jest --ci --runInBand",
        "unit:watch": "jest --watch",
        "e2e": "start-server-and-test storybook:e2e http://localhost:6006 e2e:run",
        "e2e:run": "cypress run",
        "e2e:open": "cypress open",
        "emit": "tsc",
        "prelint": "mkdir -p output/reports/tslint",
        "lint": "tslint -p tsconfig.json -t junit -o output/reports/tslint/results.xml",
        "lint:fix": "npm run lint -- --fix",
        "lint:format": "prettier --write '{src,cypress,stories,.storybook,.circleci}/**/*.{ts,tsx,js,json,xml,yml}'",
        "storybook": "start-storybook -p 6006",
        "storybook:e2e": "start-storybook -p 6006 --ci",
        "storybook:build": "build-storybook"
    },
    "pre-commit": [
        "emit",
        "lint"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/mendixlabs/time-picker"
    },
    "devDependencies": {
        "@cypress/webpack-preprocessor": "^4.0.2",
        "@storybook/addon-a11y": "^4.1.1",
        "@storybook/addon-actions": "^4.1.1",
        "@storybook/addon-knobs": "^4.1.1",
        "@storybook/addons": "^4.1.1",
        "@storybook/react": "^4.1.1",
        "@types/jest": "^23.3.10",
        "@types/node": "^10.12.15",
        "@types/react": "^16.7.17",
        "@types/react-dom": "^16.0.11",
        "@types/react-test-renderer": "^16.0.3",
        "@types/storybook__addon-a11y": "^3.3.1",
        "@types/storybook__addon-actions": "^3.4.1",
        "@types/storybook__addon-knobs": "^3.4.1",
        "@types/storybook__react": "^4.0.0",
        "copy-webpack-plugin": "^4.6.0",
        "css-loader": "^2.0.1",
        "cypress": "^3.1.3",
        "fork-ts-checker-webpack-plugin": "^0.5.2",
        "fs-extra": "^7.0.1",
        "gulp": "^3.9.1",
        "gulp-help": "^1.6.1",
        "gulp-sequence": "^1.0.0",
        "gulp-watch": "^5.0.1",
        "gulp-zip": "^4.2.0",
        "jest": "^23.6.0",
        "jest-junit": "^5.2.0",
        "mendix-client": "^7.15.6",
        "mendix-widget-build-script": "^1.0.3",
        "moment-locales-webpack-plugin": "^1.0.7",
        "pre-commit": "^1.2.2",
        "prettier": "^1.15.3",
        "raw-loader": "^0.5.1",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-hot-loader": "^4.6.0",
        "react-test-renderer": "^16.6.3",
        "start-server-and-test": "^1.7.11",
        "style-loader": "^0.23.1",
        "ts-jest": "^23.10.5",
        "ts-loader": "^5.3.1",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.4.0",
        "typescript": "^3.2.2",
        "webpack": "^4.27.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10"
    },
    "dependencies": {
        "moment": "^2.23.0",
        "react-datetime": "^2.16.3"
    }
}
