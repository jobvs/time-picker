{
  "name": "timepicker",
  "widgetName": "TimePicker",
  "version": "1.0.0",
  "description": "Time picker widget",
  "copyright": "Mendix 2018",
  "author": "Job",
  "config": {
    "widgetPath": "..//widgets",
    "projectPath": "..//"
  },
  "packagePath": "mendix",
  "scripts": {
    "start": "webpack-dev-server --hot --open",
    "emit": "tsc",
    "pretest": "tsc && npm run lint && npm run build",
    "test": "npm run test:unit && npm run test:conditional",
    "test:conditional": "node ./node_modules/mendix-widget-build-script/dist/CheckPullRequestFromFork.js && echo 'Skip tests on forked pull request' || npm run deployAndTest",
    "test:dev": "karma start",
    "test:unit": "karma start --single-run --codeCoverage && node tests/remap.js",
    "test:e2e": "tsc -p ./tests/e2e/tsconfig.json && wdio ./tests/e2e/wdio.conf.js",
    "test:e2e:dev": "tsc -p ./tests/e2e/tsconfig.json && set DEBUG=true && wdio ./tests/e2e/wdio.conf.js",
    "lint": "tslint -c tslint.json '**/*.ts' '**/*.tsx' --exclude '**/node_modules/**'",
    "dev": "gulp --gulpfile Gulpfile.js",
    "build": "NODE_ENV=production gulp build --gulpfile Gulpfile.js",
    "lint:fix": "npm run lint -- --fix",
    "deploy": " npm run updateProject && npm run deployApp",
    "updateProject": "node ./node_modules/mendix-widget-build-script/dist/UpdateMxProject.js",
    "deployApp": "node ./node_modules/mendix-widget-build-script/dist/DeployMxApp.js",
    "deployAndTest": "npm run deploy && npm run test:e2e"
  },
  "pre-commit": [
    "emit",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Job/timepicker"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/core-js": "^2.5.0",
    "@types/webdriverio": "^4.13.0",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/globalize": "0.0.34",
    "@types/jasmine": "^3.3.0",
    "@types/karma": "^3.0.0",
    "@types/node": "^10.11.7",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "check-dependencies": "^1.1.0",
    "copy-webpack-plugin": "^4.5.3",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.13",
    "fs-extra": "^7.0.0",
    "gulp": "^3.9.1",
    "gulp-copy": "^4.0.1",
    "gulp-help": "^1.6.1",
    "gulp-sequence": "^1.0.0",
    "gulp-watch": "^5.0.1",
    "gulp-zip": "^4.2.0",
    "istanbul": "^0.4.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "json-loader": "^0.5.4",
    "json-source-map": "^0.4.0",
    "jasmine": "^3.3.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "2.0.13",
    "mendix-client": "^7.15.3",
    "mendix-widget-build-script": "^1.0.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.3",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^4.3.11",
    "react-test-renderer": "^16.6.0",
    "remap-istanbul": "^0.11.1",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.2.1",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.2.0",
    "typescript": "^3.1.2",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "source-map-support": "^0.5.9",
    "wdio-dot-reporter": "0.0.10",
    "wdio-jasmine-framework": "^5.0.0-beta.3",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.8.1",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "webworkify-webpack": "^2.1.3"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
